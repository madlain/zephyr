/dts-v1/;
#include <atmel/sam4lc4c.dtsi>

/ {
	model = "Atmel SAM4L Xplained Pro";
	compatible = "atsam4l,xpro", "atmel,sam4lc4c", "atmel,sam4l";

	chosen {
		zephyr,console = &usart1;
		zephyr,shell-uart = &usart1;
		zephyr,sram = &sram0;
		zephyr,flash = &flash0;
	};
};

&usart1 {
	status = "ok";
	compatible = "atmel,sam-usart";
	current-speed = <115200>;
};
