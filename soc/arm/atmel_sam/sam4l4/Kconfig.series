config SOC_SERIES_SAM4L4
	bool "Atmel SAM4L4 MCU"
	select CPU_CORTEX_M
	select CPU_CORTEX_M4
	select SOC_FAMILY_SAM
	select SYS_POWER_LOW_POWER_STATE_SUPPORTED
	select CPU_HAS_SYSTICK
	select ASF
	help
	  Enable support for Atmel SAM4L4 Cortex-M4 microcontrollers.
